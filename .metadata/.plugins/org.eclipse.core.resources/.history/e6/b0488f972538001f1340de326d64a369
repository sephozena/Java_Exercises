package Inheritance;

public class Employee {
	private String name;
	private int age;
	private double salary;
	private double commissionPercentage;	
	
	public String getName() {
		return this.name;
	}
	
	public int getAge() {
		return this.age;
	}
	public double getSalary() {
		return this.salary;
	}
	
	public void raiseSalary() {
		this.salary = salary*1.2;
	}
	


}


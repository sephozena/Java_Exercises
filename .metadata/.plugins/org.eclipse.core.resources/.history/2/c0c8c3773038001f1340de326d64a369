package Inheritance;

public class Person {
	
	private String name;
	private String gender;
	private String location;
	private int age;
	
	
	public Person (String name, String gender, int age, String location){
		this.name = name;
		this.gender = gender;
		this.age = age;
		this.location;
	}
	
	
	public String getName() {
		return this.name;
	}
	
	public String gender() {
		return this.gender;
	}
	
	public int getAge() {
		return this.age;
	}
	
	
}
